<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PracticeManagement.MAUI.MainPage">
 
    <ScrollView>
        <HorizontalStackLayout
            Spacing="25"
            Padding="30,0"
            VerticalOptions="Center"
            HorizontalOptions="Center">
            <Button
                Text="Clients"
                Clicked="Client_Clicked">
            </Button>
            <Button
                Text="Projects"
                Clicked="Project_Clicked">
            </Button>
            <Button
                Text="Employees"
                Clicked="Employee_Clicked">
            </Button>
            <Button
                Text="Times"
                Clicked="Time_Clicked">
            </Button>
        </HorizontalStackLayout>
    </ScrollView>

</ContentPage>
